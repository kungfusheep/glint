  "keywords": ["glint", "binary", "serialization", "decoder"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.0.0",
    "@types/node": "^18.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}